<p id="notice"><%= notice %></p>

<h1>Programming Language Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Notes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @programming_language_types.each do |programming_language_type| %>
      <tr>
        <td><%= programming_language_type.name %></td>
        <td><%= programming_language_type.description %></td>
        <td><%= programming_language_type.notes %></td>
        <td><%= link_to 'Show', programming_language_type %></td>
        <td><%= link_to 'Edit', edit_programming_language_type_path(programming_language_type) %></td>
        <td><%= link_to 'Destroy', programming_language_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Programming Language Type', new_programming_language_type_path %>
